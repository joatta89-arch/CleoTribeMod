<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="ApparelMakeableBase">
    
    <defName>CuriosityBot_TigerPanties</defName>
    <label>호랑이 무늬 팬티</label>
    <description>용맹한 호랑이의 기운이 느껴지는 팬티입니다. 어째선지 용기가 솟아나는 것 같습니다.</description>
	  <techLevel>Neolithic</techLevel>
	  
    <graphicData>
      <texPath>Things/Apparel/TigerPanties_Icon</texPath> 
      <graphicClass>Graphic_Single</graphicClass>
      <color>(255, 255, 255)</color>
    </graphicData>

   <stuffCategories>
   <li>Leathery</li>
   </stuffCategories>

   <costStuffCount>50</costStuffCount>

  <statBases>
   <MaxHitPoints>100</MaxHitPoints>
   <MarketValue>50</MarketValue>
   <Mass>0.1</Mass>
   <EquipDelay>1.0</EquipDelay>
   <WorkToMake>2000</WorkToMake> 
   
   <ArmorRating_Blunt>0.05</ArmorRating_Blunt>
   <ArmorRating_Sharp>0.025</ArmorRating_Sharp>
   <Insulation_Cold>0.5</Insulation_Cold>
   <Insulation_Heat>0.2</Insulation_Heat>
   <StuffEffectMultiplierArmor>0.20</StuffEffectMultiplierArmor>
   <StuffEffectMultiplierInsulation_Cold>0.5</StuffEffectMultiplierInsulation_Cold>
   <StuffEffectMultiplierInsulation_Heat>0.5</StuffEffectMultiplierInsulation_Heat>
   <Insulation_Heat>3.0</Insulation_Heat>
   <MoveSpeed>0.30</MoveSpeed>
  </statBases>

  <recipeMaker>
    <researchPrerequisite>ComplexClothing</researchPrerequisite>
	  <recipeUsers>
		  <li>CuriosityBot_Spot</li>
		  <li>CraftingSpot</li>
	  </recipeUsers>
  </recipeMaker>

    
  <apparel>

	  <tags>
		  <li>Neolithic</li>
		  <li>Cleo_Apparel</li>
		  <li>CleoApparelOnly</li>
	  </tags>
	  
     <bodyPartGroups> <li>Legs</li> </bodyPartGroups>
     <layers> <li>OnSkin</li> </layers>
 	 <countsAsClothingForNudity>false</countsAsClothingForNudity>
     <wornGraphicPath>Things/Apparel/TigerPanties/TigerPanties</wornGraphicPath>
	  <useWornGraphicMask>false</useWornGraphicMask>
  </apparel>

	  <thingCategories>
	    	<li>Apparel</li>
	  </thingCategories>

	  <colorGenerator Class="ColorGenerator_Options">
		  <options>
			  <li>
				  <weight>0.05</weight>
				  <only>(255,255,255)</only>
			  </li>
			  <li>
				  <weight>0.10</weight>
				  <only>(255,255,255)</only>
			  </li>
			  <li>
				  <weight>0.15</weight>
				  <only>(255,255,255)</only>
			  </li>
		  </options>
	  </colorGenerator>
	  
	  
  </ThingDef>
	<ThingDef ParentName="ApparelMakeableBase">

